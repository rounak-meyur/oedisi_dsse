            "type": "LinDistEstimator",
            "parameters": {
                "algorithm_parameters": {"tol": 1e-5}
            }
        },
        {
            "name": "local_feeder",
            "type": "Feeder",
            "parameters": {
                "user_uploads_model": false,
                "use_smartds": false,
